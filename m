+++ linux-pm/drivers/thermal/thermal_core.c
@@ -1394,7 +1394,6 @@ unregister:
 	device_del(&tz->device);
 release_device:
 	put_device(&tz->device);
-	tz = NULL;
 remove_id:
 	ida_free(&thermal_tz_ida, id);
